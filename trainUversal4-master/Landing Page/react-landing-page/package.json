{
  "name": "react-landing-page",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.11.2",
    "@material-ui/core": "^4.9.13",
    "@material-ui/icons": "^4.9.1",
    "bootstrap": "^4.4.1",
    "mdbreact": "^4.25.6",
